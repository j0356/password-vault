# ============================================
# PASSWORD VAULT - DOCKER ENVIRONMENT
# ============================================
# Copy this file to .env before running docker-compose
# IMPORTANT: Change all default values in production!

# Database Configuration
MYSQL_ROOT_PASSWORD=root_password_change_me
DB_NAME=password_vault
DB_USER=vault_user
DB_PASSWORD=vault_secure_password

# JWT Configuration
JWT_SECRET=change-this-super-secret-jwt-key-in-production-minimum-32-chars
JWT_EXPIRES_IN=24h

# Encryption Key (must be exactly 32 characters for AES-256)
ENCRYPTION_KEY=change-this-32-char-encryption!
